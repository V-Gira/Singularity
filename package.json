{
  "name": "singularity",
  "version": "1.0.0",
  "description": "A campaign for Minutes to Midnight - Website built on Fari.games",
  "homepage": "https://singularity-campaign.netlify.app",
  "author": "V-Jira",
  "license": "AGPL-3.0",
  "main": "index.js",
  "engines": {
    "node": "^16.2.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "validate": "npm run lint && npm run tsc",
    "serve": "vite build && vite preview",
    "lint": "eslint 'src/**' --cache",
    "preview": "npm run build && npm run deploy:preview",
    "beta": "npm run build && npm run deploy:preview -- --alias beta",
    "alpha": "npm run build && npm run deploy:preview -- --alias alpha",
    "serve:build": "vite preview --port 1234",
    "prod": "npm run build && npm run deploy:preview -- --prod",
    "deploy:preview": "netlify deploy --site 7f652f35-3f42-49b3-a232-a5a979dd9fd0",
    "deploy:prod": "netlify deploy --site 7f652f35-3f42-49b3-a232-a5a979dd9fd0 --prod",
    "tsc": "tsc --project tsconfig.json --incremental --noEmit"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "immer": "^9.0.2",
    "lodash": "^4.17.21",
    "i18next": "^20.3.0",
    "i18next-browser-languagedetector": "^6.1.1",
    "i18next-http-backend": "^1.2.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-i18next": "^11.9.0",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.28.1",
    "remarkable": "^2.0.1",
    "slick-carousel": "^1.8.1",
    "netlify-cli": "^3.31.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.171",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-slick": "^0.23.4",
    "@types/remarkable": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-legacy": "^1.5.1",
    "@vitejs/plugin-react": "^1.1.1",
    "prettier": "^2.3.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "typescript": "^4.4.2",
    "vite": "^2.5.0"
  }
}
